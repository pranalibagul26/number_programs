import java.util.*;

class Harshad_num
{
public static void main(String[]args)
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int sum=0;
int n1=n;

while(n>0)
{
int rem=n%10; 
sum=sum+rem;
n=n/10;
}
if(n1%sum==0)
{
System.out.println("harshad number");
}
else
{
System.out.println("not a harshad numb");
}
}
}
